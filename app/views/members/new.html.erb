
<% provide(:title, 'Sign up') %>
<h1>Sign up</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for( @member ) do |f| %>
      <p><%= f.label :name %>
      <%= f.text_field :fname, style:"color: black;"%></p>

      <p><%= f.label :email %>
      <%= f.email_field :email, style:"color: black;"%></p>
      <%= f.submit "Sign up", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
